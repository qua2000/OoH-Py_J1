      "source": [
        "#sample\n",
        "\n",
        "x = input(\"身長を入れて下さい(単位ｍ) : \")\n",
        "\n",
        "y = input(\"体重を入れて下さい(単位㎏) : \")\n",
        "\n",
        "bmi = float(y) / float(x)**2\n",
        "\n",
        "bmi = round(bmi,1)\n",
        "\n",
        "print(\"あなたの身長は\", x, \"mで，体重は\", y, \"kgなので，BMIは，\", bmi, \"です。\")\n"
