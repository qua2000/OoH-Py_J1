        "#Fractionを使うSample\n",
        "\n",
        "from fractions import Fraction\n",
        "\n",
        "count=0\n",
        "sum = 0\n",
        "\n",
        "for n in range(1,45):\n",
        "  if n % 3 == 0 or n % 5 == 0:\n",
        "    count += 1\n",
        "  if n % 3 != 0 and n % 5 != 0:\n",
        "    sum += Fraction(n,45)\n",
        "\n",
