      "source": [
        "\n",
        "def Fibo(n):\n",
        "    if n < 3:\n",
        "        return 1\n",
        "    return Fibo(n-1) + Fibo(n-2)\n",
        "\n",
        "for i in range(10):\n",
        "    print(Fibo(i+1), end=', ')\n"
      ],
      "execution_count": null,
