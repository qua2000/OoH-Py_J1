    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Kp_2QOdK8DQs"
      },
      "source": [
        "#<Code sample>\n",
        "\n",
        "number = 1500\n",
        "\n",
        "for x in range(1500, 2500):\n",
        "  if (x + 6) % 11 == 0 and (x + 11) % 6 == 0 and abs(2017 - number) > abs(2017 - x):\n",
        "      number = x\n",
        "      print(number)    #このコードを入れておくとnumberに入っている数が変わっていくのがわかる\n",
        "\n",
        "print(number, \"・・・(答)\")\n",
        "    "
