      "source": [
        "#リスト (要素の追加・削除)\n",
        "\n",
        "Mylist = [\"a\", \"b\", \"c\"]\n",
        "\n",
        "print(Mylist)\n",
        "\n",
        "print(\"b\" in Mylist)\n",
        "\n",
        "print(\"d\" in Mylist)\n",
        "\n",
        "Mylist.append(\"d\")\n",
        "\n",
        "print(Mylist)\n",
        "\n",
        "Mylist.remove(\"a\")\n",
        "\n",
        "print(Mylist)\n",
