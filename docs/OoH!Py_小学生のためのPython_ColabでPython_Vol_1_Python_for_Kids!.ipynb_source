        "#@title\n",
        "week = ['日曜日','月曜日','火曜日','水曜日','木曜日','金曜日','土曜日']\n",
        "\n",
        "year,month,day = (int(x) for x in input('生年月日を半角で，西暦年 月 日で入力(スペースも半角)>').split())\n",
